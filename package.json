{
  "name": "boilerplate-node-express-api",
  "version": "1.0.0",
  "description": "Boilerplate APIs with express",
  "main": "index.js",
  "scripts": {
    "start": "export DEBUG=app:*||set DEBUG=app:*&&nodemon src/app.js --exec babel-node -e js",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "prod": "export NODE_ENV=production||set NODE_ENV=production&&npm run build&&node dist/app.js"
  },
  "keywords": [],
  "author": "Somprasong Damyos",
  "license": "MIT",
  "dependencies": {
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "fawn": "^2.1.5",
    "helmet": "^3.12.1",
    "joi": "^13.4.0",
    "joi-objectid": "^2.0.0",
    "mongoose": "^5.2.1",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "swagger-ui-express": "^3.0.10",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.17.5"
  }
}
