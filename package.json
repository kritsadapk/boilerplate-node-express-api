{
  "name": "boilerplate-node-express-api",
  "version": "1.0.0",
  "description": "Boilerplate APIs with express",
  "main": "index.js",
  "scripts": {
    "start": "export DEBUG=app:*||set DEBUG=app:*&&nodemon bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "serve": "export NODE_ENV=production||set NODE_ENV=production&&node dist/app.js",
    "prod": "export NODE_ENV=production||set NODE_ENV=production&&npm run build && node bin/prod"
  },
  "keywords": [],
  "author": "Somprasong Damyos",
  "license": "MIT",
  "dependencies": {
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.17.5"
  }
}
